{
  "name": "ActivityTracker",
  "description": "Track and analyze your activity",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["storage", "activeTab", "tabs"],
  "options_ui": {
    "page": "./options.html"
  },
  "background": {
    "scripts": ["./background.min.js"]
  },
  "browser_action": {
    "default_icon": "./images/icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@activity-tracker-ext.com",
      "strict_min_version": "42.0"
    }
  }
}
